<!DOCTYPE html>

<div class="row">
	<form role="form" type="get" action="/dardashat-attendance/">
	    <div class="col-lg-12">
	        <div class="panel panel-default">
	            <div class="panel-heading">
	               Semester Data
	            </div>
	            <div class="panel-body">
	                <div class="row">
	                    <div class="col-lg-6">
                            <div class="form-group">
								<label>Semester</label>
								<select class="form-control">
									<option value="Nationality">First Semester</option>
									<option value="Nationality">Second Semester</option>
									<option value="Nationality">Summer Semester</option>	
								</select>
							</div>
							<div class="form-group">
								<label>Year</label>
								<input type="dateyear" name="year"  class="form-control">
							</div>
							 <div class="form-group">
								<label>Scholar</label>
								<select class="form-control">
									<option value="Nationality">All current scholars</option>
	
								</select>
							</div>
							<!-- 1+2 = 100% -->
							<div class="form-group">
								<label>Semester GPA</label>						
								<input type="number" class="form-control">
							</div>
							
							<div class="form-group">
								<label>GPA</label>						
								<input type="number" class="form-control">
							</div>
						</div>
						<div class="col-lg-6">
							<div class="form-group">
								<label>Registered Hours</label>							
								<input type="number" class="form-control">
							</div>
							<div class="form-group">
								<label>Accounted Hours Passed</label>						
								<input type="number" class="form-control">
							</div>
							<div class="form-group">
								<label>Unaccounted Hours Passed</label>						
								<input type="number" class="form-control">
							</div>
							<div class="form-group">
								<label>Failed Hours</label>						
								<input type="number" class="form-control">
							</div>
							<div class="form-group">
								<label>Dropped Hours</label>							
								<input type="number" class="form-control">
							</div>
	                    </div>
	                    <!-- /.col-lg-6 (nested) -->
	                </div>
	                <!-- /.row (nested) -->
	            </div>
	            <!-- /.panel-body -->
	        </div>
	        <!-- /.panel -->
	    </div>
	   <!-- /.col-lg-12 -->
   </form>
</div>
 <!-- /.row -->
